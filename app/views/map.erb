<!DOCTYPE html>
<html>
  <head>
  	<h1>Welcome to Plot.</h1>
    <style>
      #map {
        height: 400px;
        width: 100%;
       }
    </style>
  </head>
  <body>
    <h3>Coming Soon!</h3>
    <div id="map"></div>
    <script>
      function initMap() {
        var uluru = {lat: -25.363, lng: 131.044};
        var map = new google.maps.Map(document.getElementById('map'), {
          zoom: 4,
          center: uluru
        });
        var marker = new google.maps.Marker({
          position: uluru,
          map: map
        });
      }
    </script>
    <script async defer
    src="https://maps.googleapis.com/maps/api/js?AIzaSyC8LnY1K43gLr-t8EUHGf7EB-EXxmiZTbQ&callback=initMap">
    </script>
  </body>
</html>